#include<stdio.h>
#include<string.h>
int main()
{
    char str[20];
    scanf("%s",str);
    char lo;
    scanf(" %c",&lo);
    int l,j;
     l=strlen(str);
     
        for(j=l-1;j>=0;j--)
        {
        if(str[j]==lo)
        {
            printf("%d",j+1);
            break;
        }
         
    }
   return 0;
}


