#include<stdio.h>
#include<string.h>
int main()
{
    char a[29];
    gets(a);
    int c=strlen(a);
    int i=0,j,k;
    while(i<c)
    {
        j=i+1;
        while(a[j]!='\0')
        {
            if(a[j]==a[i])
            {
                k=j;
                while(a[k]!='\0')
                {
                    a[k]=a[k+1];
                    k++;
                }
            }
            j++;
        }i++;
    }
    printf("%s",a);


    
}
