
#include<stdio.h>
#include<string.h>
int main()
{
    char str[20];
    scanf("%s",str);
    char lo;
    scanf(" %c",&lo);
    int l,j;
     l=strlen(str);
     
        for(j=0;j<l;j++)
        {
        if(str[j]==lo)
        {
            printf("%d\n",j+1);
            
        }
         
    }
   return 0;
}


