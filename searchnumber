 #include<stdio.h>
 int main()
 {
     int n,i;
     scanf("%d",&n);
     int a[n];
     for(int i=0;i<n;i++)
     {
         scanf("%d",&a[i]);
     }
     int search;
     scanf("%d",&search);
     for(int i=0;i<n;i++)
     {
         if(search==a[i])
         {
             printf("%d found at position %d",search,i+1);
            
         } 
        
     }
      if(search!=a[i+1])
         {
             printf("%d not found",search);
         }
     return 0;
 }
