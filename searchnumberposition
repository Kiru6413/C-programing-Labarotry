#include<stdio.h>
int main()
{
    int n,i;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int search;
    scanf("%d",&search);
    for(int i=0;i<n;i++)
    {
        if(search==arr[i])
        {
            printf("%d Found at position %d",search,i+1);
        }
    }
    return 0;
}
